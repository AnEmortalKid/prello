{
  "manifest_version": 3,
  "name": "Prello",
  "description": "An extension for trello that displays percentage of items in a list",
  "version": "1.0",
  "action": {
    "default_icon": "icons/icon16.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://trello.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}